{
  "name": "ansiskengis/eleving-round-robin-tournament",
  "description": "Round-Robin Tournament Simulation",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "authors": [
    {
      "name": "AnsisKengis",
      "email": "ansiskengis@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "doctrine/migrations": "^3.8",
    "doctrine/orm": "^3.3",
    "fakerphp/faker": "^1.24",
    "symfony/cache": "^7.2",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0"
  },
  "scripts": {
    "migrations:diff": "vendor/bin/doctrine-migrations diff --allow-empty-diff",
    "migrations:migrate": "vendor/bin/doctrine-migrations migrate --no-interaction",
    "migrations:generate": "vendor/bin/doctrine-migrations generate",
    "migrations:status": "vendor/bin/doctrine-migrations status",
    "phpunit:run": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
